<h1>Messages#index</h1>
<p>Find me in app/views/messages/index.html.erb</p>

<% @messages.each do |message| %>
  <div>
    <div>
      <%= User.find(message.user_id).username %>
    <div>
    <div>
      <%= message.body %>
    </div>
  </div>
<% end %>

<div>
  <%= simple_form_for [@conversation, @message] do |f| %>
    <%= f.input :body, label: 'new message' %>
    <%= f.button :submit %>
  <% end %>
</div>


